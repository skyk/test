page{background: #f5f5f5}
.container{width:694rpx;height:958rpx;margin:100rpx auto 0;position: relative}
.container>image{width:694rpx;position: absolute;top:0;left: 0}
.container .content{position: relative;z-index: 1;padding-top:156rpx;text-align: center}
.container .barcode{width:614rpx;height:200rpx;background: #666;margin: 20rpx auto 0}
.container .content .portrait{overflow: hidden;width: 538rpx;margin:170rpx auto 80rpx}
.container .content .portrait view{float:left}
.container .content .portrait view.point{width:6rpx;height:12rpx;background: #FF694B;margin-top:50rpx;margin-right:52rpx}
.container .content .portrait view.point:nth-child(4){margin-right:30rpx}
.container .content .portrait view.point:last-child{margin-right:0}
.container .content .portrait .icon{width: 116rpx;height:116rpx;border-radius:50%;background-image: url('http://h5smg.miyapay.com/h5/hhxcx_img/bg_circle@2x.png');background-size:100%; margin-right:30rpx;text-align:center;padding-top:18rpx}
.container .content .portrait image{width:80rpx;height:80rpx;border-radius:50%}
.container .content .tips{font-size:30rpx;color:#6C7B8A}
.container .content .line{width:80rpx;height:6rpx;background:#FF694B;margin:35rpx auto 0}
