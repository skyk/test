.coupons .item .container .bottom {
  width: 100%;
  height: 76rpx;
  padding-left: 25rpx;
  
}

.coupons .item .container .bottom view {
  
  float: left;
}

.coupons .item .container .bottom view:first-child {
  font-size: 20rpx;
  color: #ff694b;
  /* border: 1rpx solid #ff694b; */
