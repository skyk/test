.couponsBox {
  width: 100%;
  display: flex;
  flex-flow: column;
}

.shengqian {
  width: 170rpx;
  height: 42rpx;
  ont-size: 30rpx;
  color: #000;
  margin-left: 290rpx;
  margin-top: 44rpx;
  white-space: nowrap;
}

.line {
  width: 80rpx;
  height: 6rpx;
  margin-left: 335rpx;
  margin-top: 20rpx;
}

.coupons {
  width: 100%;
  background: #f5f5f5;
  padding: 28rpx;
  margin-top: 26rpx;
  position: relative;
}

.coupons .item {
  width: 694rpx;
  height: 276rpx;
  position: relative;
  margin-bottom: 15rpx;
}

.coupons .item .bak {
  width: 694rpx;
  position: absolute;
  left: 0;
  top: 0;
}

.coupons .item .container {
  width: 100%;
  position: relative;
  z-index: 1;
  overflow: hidden;
}

.coupons .item .container .top {
  width: 76%;
  padding-top: 25rpx;
  padding-left: 25rpx;
  overflow: hidden;
  height: 200rpx;
  border-bottom: 1rpx solid #e7e8e9;
}

.coupons .item .container .top view {
  float: left;
}

.coupons .item .container .top view.icon image {
  width: 160rpx;
}

.coupons .item .container .top view.details {
  width: 340rpx;
  padding-left: 20rpx;
}

.coupons .item .container .top view.details .prod-name {
  width: 290rpx;
  font-size: 30rpx;
  color: #000;
  font-weight: bold;
  margin-top: 10rpx;
  line-height: 40rpx;
}

.coupons .item .container .top view.details .prod-coupon {
  width: 100%;
  margin-top: 15rpx;
}

.coupons .item .container .top view.details .prod-coupon view {
  float: left;
}

.coupons .item .container .top view.details .prod-coupon view:nth-child(1) {
  width: 244rpx;
}

