}
.finish-btn {
  width: 600rpx;
  height: 100rpx;
  font-size: 30rpx;
  text-align: center;
  line-height: 100rpx;
  border-radius: 15rpx;
  margin: 80rpx auto 30rpx;
  color: #fff;
  background: linear-gradient(to right, #ff694b, #ff9b7d);
  box-shadow: 0 0 20rpx #ff9b7d;
}
.text-count{
  position: absolute;
  right: 0rpx;
  top:330rpx;
}
.exceed{
  color: #FF694B;
}
